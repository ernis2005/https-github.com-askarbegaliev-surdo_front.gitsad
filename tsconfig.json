{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "components/Form/Redister/Register.jsx",
    "components/Form/Login/Login.jsx",
    "components/Form/Confirmation/Confirmation.jsx",
    "components/Header/Header.jsx",
    "app/page/news/page.jsx",
    "components/Cards/NewCards/NewCards.jsx",
    "app/page/news/[id]/page.jsx",
    "app/getData/getData.js",
    "app/page/questions/page.jsx",
    "app/page.jsx",
    "components/Accordion/Accordion.jsx",
    "app/store/ProviderRedux/ProviderRedux.jsx",
    "app/page/profil/page.jsx"
  ],
  "exclude": ["node_modules"]
}
